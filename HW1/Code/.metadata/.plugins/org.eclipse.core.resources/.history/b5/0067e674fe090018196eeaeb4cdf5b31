import random
N = 0

def getFilePath():
    import tkinter as tk
    from tkinter import filedialog
    global fileNew
    root = tk.Tk()
    root.withdraw()
    fileNew = filedialog.askopenfilename()
    return fileNew

def genRandomArray():
    global N
    a = []
    for i in range(0, N-1):
        a.append(round(100*random.random(), 2))
    
    print(a)  


